{
  "name": "exobuilder-gui",
  "version": "1.0.1",
  "description": "A simple graphical interface for exobuilder, made with electron js.",
  "main": "index.js",
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-dmg": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/publisher-github": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "electron": "^31.4.0"
  },
  "scripts": {
    "test-linux": "APP_DEV=true electron .",
    "test-windows": "set APP_DEV=true && electron .",
    "test-prod": "electron .",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "env-cmd electron-forge publish"
  },
  "build": {
    "extraResources": [
      {
        "from": "./libs/",
        "to": "libs",
        "filter": [
          "**/*"
        ]
      }
    ],
    "mac": {
      "icon": "resources/libs/Ex0sArc4d3P1C.icns"
    },
    "win": {
      "icon": "resources/libs/Ex0sArc4d3P1C.ico"
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": true,
        "extraResource": [
          "./libs"
        ],
        "icon": "./libs/Ex0sArc4d3P1C"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip"
        },
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "exobuilder-gui"
          }
        },
        {
          "name": "@electron-forge/maker-deb"
        },
        {
          "name": "@electron-forge/maker-rpm"
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "background": "./libs/Ex0sArc4d3P1C.png",
            "format": "ULFO"
          }
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "Xarber",
              "name": "exobuilder-gui"
            },
            "prerelease": false,
            "draft": true
          }
        }
      ]
    }
  },
  "author": "Xarber",
  "license": "ISC",
  "dependencies": {
    "@electron-delta/updater": "^0.1.17",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.2.1",
    "env-cmd": "^10.1.0"
  }
}
